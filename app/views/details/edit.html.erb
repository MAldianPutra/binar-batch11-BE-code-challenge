<h3>Update Details</h3>
<%= form_with model: @detail do |form| %>
  <%= form.text_field :item_id, placeholder: "item_id" %>
  <%= form.text_field :order_id, placeholder: "order_id" %>
  <%= form.text_field :price, placeholder: "detail_price" %>
  <%= form.text_field :quantity, placeholder: "detail_quantity"%>
  <%= form.submit %>
<% end %>