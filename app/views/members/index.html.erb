<h1> List of Members </h1>
<table>
  <% @members.each do |member| %>
    <tr>
      <td><%= link_to member.member_name, member_path(member) %></td>
      <td><%= button_to 'Edit', edit_member_path(member), method: 'get'%></td>
      <td><%= button_to 'Remove', member_path(member), method: 'delete', data: { confirm: 'Are you sure?' } %></td>
  <% end %>
</table>
<%= link_to "Add a New Member", new_member_path %>